  
#include <iostream>
#include<string>
using namespace std;

class student
{
    public:
    string name;
    int per;
    
    public:
    student()
    {
         name="Renish";
         per=95;
    }
   
    
    int output()
    {
        cout<<"\n Name:"<<name;
        cout<<"\n Percentage:"<<per;
        cout<<"%";
        return 0;
    }
};





int main() 
{
   
    student s1;
    
    cout<<"\n Student info:";
     
    s1.output();
     
     return 0;
}
