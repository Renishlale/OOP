 #include <iostream>
#include<string>
using namespace std;
 
class summation 
{   public:
    int n;
    int sum=0;
    public:
    summation()
    {
       n=10;
        
      }
    int output()
    {
          for( int i=1;i<=n;i++)
        {
            sum=sum+i;
        }
        
        cout<<"Sum:"<<sum;
        return 0;
    }
};
    
    
    
    int main()
    {
        summation s1;
        s1.output();  
        
        
    }
    
    
    
