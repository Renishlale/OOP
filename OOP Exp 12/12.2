 #include <iostream>
using namespace std;

template <class T>
class Queue {
    T q[10];
    int front, rear;

public:
    Queue() {
        front = -1;
        rear = -1;
    }

    void insert(T x) {
        if (rear == 9) {
            cout << "Queue is full. Cannot insert " << x << endl;
        } else {
            if (front == -1) front = 0; // first element
            q[++rear] = x;
            cout << "Inserted: " << x << endl;
        }
    }

    void remove() {
        if (front == -1 || front > rear) {
            cout << "Queue is empty. Cannot remove." << endl;
        } else {
            cout << "Removed: " << q[front++] << endl;
        }
    }

    void display() {
        if (front == -1 || front > rear) {
            cout << "Queue is empty." << endl;
        } else {
            cout << "Queue elements: ";
            for (int i = front; i <= rear; i++) {
                cout << q[i] << " ";
            }
            cout << endl;
        }
    }
};

int main() {
    Queue<int> q;

    q.insert(10);
    q.insert(20);
    q.insert(30);
    q.display();

    q.remove();
    q.display();

    return 0;
}
