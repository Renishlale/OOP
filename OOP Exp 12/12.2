 #include <iostream>
#include <queue>
using namespace std;

template <class T>
class MyQueue {
    queue<T> q;

public:
    void enqueue(T value) {
        q.push(value);
        cout << value << " added to queue.\n";
    }

    void dequeue() {
        if (q.empty()) {
            cout << "Queue is empty.\n";
        } else {
            cout << q.front() << " removed from queue.\n";
            q.pop();
        }
    }

    void display() {
        if (q.empty()) {
            cout << "Queue is empty.\n";
            return;
        }
        queue<T> temp = q; // copy to display
        cout << "Queue elements: ";
        while (!temp.empty()) {
            cout << temp.front() << " ";
            temp.pop();
        }
        cout << endl;
    }
};

int main() {
    MyQueue<int> q;
    q.enqueue(100);
    q.enqueue(200);
    q.display();
    q.dequeue();
    q.display();
    return 0;
}
