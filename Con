#include <iostream>
using namespace std;
 
class college 
{
    int roll_no;
    char name[50];
    char course[50];

 
college( int roll_no1,char name1[50],course="Computer_science")
 {
     roll_no=roll_no1;
     name=name1;
 }
 
 int input()
 {
    
     printf("\n Name:",name);
     printf ("\n Roll no.:",roll_no);
     printf("\n Course:",course);
     return 0;
 }
};
 
 
int main() 
{
   college c1(16,Renish);
   c1(16,Renish).input();

    return 0;
}
